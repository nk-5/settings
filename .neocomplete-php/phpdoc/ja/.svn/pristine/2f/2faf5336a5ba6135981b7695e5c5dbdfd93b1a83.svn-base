<refentry xmlns="http://docbook.org/ns/docbook" xml:id="function.sybase-get-last-message">
 <refnamediv>
  <refname>sybase_get_last_message</refname>
  <refpurpose>サーバーから直近のメッセージを返す</refpurpose>
 </refnamediv>
 
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>string</type><methodname>sybase_get_last_message</methodname>
   <void/>
  </methodsynopsis>
  <para>
   <function>sybase_get_last_message</function>
   はサーバーからの直近のメッセージを返します。
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   メッセージを文字列で返します。
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>sybase_min_message_severity</function></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

